                       <script>
                       function parcelamento() {
                        if($('.price_range ins').length > 0){
                          var preco = $('.price_range ins').text().replace("R$ ", "").replace(',', '.')  
                        }else{
                          var preco = $('span.price_varies.current_price').text().replace("R$ ", "").replace(',', '.')
                        }   
                        var compare = $('.price_range del').text().replace('R$ ', '').replace(',','.')
                        var compare = parseFloat(compare)
                        var precompare = (compare - preco).toFixed(2).replace('.', ',')
                        $('#shopify-section-pr_summary .product-label.product-label--on-sale span').text('R$ '+ precompare)
                        var porcent = ((compare - preco) * 100 / compare).toFixed(2).split('.')[0]
                        $('#shopify-section-pr_summary .precocheio .product-label.product-label--on-sale').empty()
                        $('#shopify-section-pr_summary .price--highlight .product-label.product-label--on-sale').append( porcent + '% OFF')    
                        var preco = parseFloat(preco);
                       {% if settings.appmax %}
                        var calculo = ((preco + 0) * 1.2048) / 12;
                       {% elsif settings.cartpanda %}
                        var calculo = ((preco + 0) * 1.2375) / 12;
                       {% elsif settings.personalizado %}
                        var calculo = ((preco + 0) * {{juros}}) / 6;
                       {% endif %}
                        var calculo = calculo.toFixed(2).replace('.', ',');
                        var calculo = ('R$ ' + calculo);
                        $('#shopify-section-pr_summary .parcelamento').html('<span><h>em até</h> 6x de <b>' + calculo + '</b></span> sem juros');
                        }
                      
                      
                        $(".nt-swatch").click(function () {
                            setTimeout(function () { parcelamento(); }, 150);
                        });
